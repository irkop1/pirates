$dua = $js["2"]; // 20 hari  b1ab8840120c380c7b26504ddb4fb26b
$tiga = $js["3"]; // 30 hari 926e21bd2bd55e5dea4343ec1564f169

if($key == $satu):
$ss = date("d-m-Y", strtotime("+11 days",time()));
$key = '<?php
$keys = array();
$keys [] = "'.$key.'";
//';
file_put_contents($aks,$key);
